d7 $ striate 10 $ s "notes(5,9)" # n (range 3 8 cosine)
d1 $ s "bd bd [bd*3] bd"
d8 $ jux (rev) $ s "909*8" # speed (choose [-1..5])
solo 4
unsolo 4
solo 3
xfade 3 $ s "~"
hush
